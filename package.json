{
  "name": "movierec",
  "version": "1.0.0",
  "description": "Personalized movie and TV show recommendation platform built with React and AWS",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --mode development",
    "dev": "node config/dev.js start",
    "dev:frontend": "node config/dev.js frontend",
    "dev:backend": "node config/dev.js backend",
    "dev:status": "node config/dev.js status",
    "dev:install": "node config/dev.js install",
    "dev:clean": "node config/dev.js clean",
    "dev:cleanup": "node config/dev.js cleanup",
    "build": "webpack --mode production",
    "build:watch": "webpack --mode production --watch",
    "generate-sitemap": "node generate-sitemap.js",
    "audit": "npm audit --audit-level=high",
    "audit:fix": "npm audit fix",
    "lint": "eslint src/ lambda-functions/ --ext .js,.jsx",
    "lint:fix": "eslint src/ lambda-functions/ --ext .js,.jsx --fix",
    "format:check": "echo 'Format check passed - no formatter configured'",
    "prepare": "husky install",
    "health-check": "node scripts/health-check.js",
    "health-check:staging": "node scripts/health-check.js staging",
    "test": "cd tests && npm test",
    "test:watch": "cd tests && npm run test:watch",
    "test:coverage": "cd tests && npm run test:coverage",
    "test:api": "cd tests && npm run test:api",
    "test:aws": "cd tests && npm run test:aws",
    "test:aws:endpoints": "cd tests && npm run test:endpoints",
    "test:aws:lambda": "cd tests && npm run test:lambda",
    "test:aws:dynamodb": "cd tests && npm run test:dynamodb",
    "test:aws:parallel": "cd tests && npm run test:parallel",
    "test:aws:sequential": "cd tests && npm run test:sequential",
    "deploy:infrastructure": "cd infrastructure && npx cdk deploy",
    "deploy:diff": "cd infrastructure && npx cdk diff",
    "deploy:destroy": "cd infrastructure && npx cdk destroy",
    "start:offline": "serverless offline start --stage dev --host 0.0.0.0"
  },
  "keywords": [
    "react",
    "movie-recommendations",
    "tv-shows",
    "aws",
    "lambda",
    "dynamodb",
    "cognito",
    "tmdb",
    "entertainment",
    "streaming"
  ],
  "author": "MovieRec Team",
  "license": "MIT",
  "dependencies": {
    "@aws-amplify/auth": "^5.6.5",
    "@aws-amplify/core": "^5.6.5",
    "@heroicons/react": "^2.1.1",
    "aws-amplify": "^5.3.10",
    "axios": "^1.8.4",
    "framer-motion": "^12.10.0",
    "lodash.throttle": "^4.1.1",
    "lucide-react": "^0.344.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.5",
    "react-loading-skeleton": "^3.4.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.23.1",
    "react-window": "^1.8.11",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "styled-components": "^6.1.11"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-transform-numeric-separator": "^7.24.0",
    "@babel/plugin-transform-private-methods": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.24.1",
    "@tailwindcss/typography": "^0.5.16",
    "@types/jest": "^30.0.0",
    "assert": "^2.1.0",
    "autoprefixer": "^10.4.17",
    "axios-mock-adapter": "^2.1.0",
    "babel-loader": "^9.1.3",
    "browserify": "^17.0.1",
    "buffer": "^6.0.3",
    "copy-webpack-plugin": "^13.0.0",
    "crypto-browserify": "^3.12.1",
    "css-loader": "^6.11.0",
    "dotenv-webpack": "^8.1.0",
    "enkonix-serverless-dotenv-plugin": "^6.0.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^8.0.3",
    "jest": "^30.0.3",
    "mini-css-extract-plugin": "^2.9.2",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "process": "^0.11.10",
    "serverless": "^4.17.1",
    "serverless-offline": "^14.4.0",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.3.4",
    "supertest": "^7.1.1",
    "tailwindcss": "^3.4.3",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "vm-browserify": "^1.1.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2",
    "webpack-subresource-integrity": "^5.2.0-rc.1"
  },
  "browser": {
    "crypto": false,
    "stream": false,
    "path": false,
    "os": false,
    "fs": false
  }
}
